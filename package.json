    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "curl -s https://get.simplelocalize.io/install | bash",
    "build": "gatsby build",
    "prebuild": "run-p \"prebuild:*\"",
    "prebuild:graphql": "yarn graphql",
    "prebuild:i18n": "yarn i18n",
    "develop": "gatsby develop --host 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn prebuild && yarn run develop",
    "start:dev": "ENABLE_GATSBY_REFRESH_ENDPOINT=1 yarn run start",
    "refresh": "curl -d '' http://localhost:8000/__refresh",
    "refresh:dev": "while true; do yarn refresh; sleep 5; done",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gh-pages -d docs",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "graphql": "run-s \"graphql:*\"",
    "graphql:schema": "env-cmd get-graphql-schema $GRAPHCMS_URL > src/config/schema.graphql",
    "graphql:types": "graphql-codegen --config codegen.yml",
    "i18n": "run-s \"i18n:*\"",
    "i18n:extract": "run-s \"i18n:extract:*\"",
    "i18n:extract:run": "yarn formatjs extract 'src/**/!(*.d).ts*' --out-file lang.json --format simple",
    "i18n:extract:print": "echo 'Exported Translations:' && cat lang.json",
    "i18n:upload": "env-cmd --use-shell simplelocalize upload --apiKey '$SIMPLELOCALIZE_KEY' --uploadPath lang.json --uploadFormat simplelocalize-json --languageKey en",
    "i18n:download": "env-cmd --use-shell simplelocalize download --apiKey '$SIMPLELOCALIZE_KEY' --downloadFormat single-language-json --downloadPath ./src/i18n/{lang}.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "resolutions": {
    "gatsby-source-graphql-universal": "3.3.0"
  }
}
